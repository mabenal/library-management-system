{
  "runtime": "Net90",
  "documentGenerator": {
    "fromDocument": {
      "url": "swagger.json"
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClient": {
      "className": "{controller}Client",
      "clientBaseClass": "I{controller}Client",
      "generateDtoTypes": false,
      "importRequiredTypes": true,
      "baseUrlTokenName": "API_BASE_URL",
      "output": "auto/autolmsclient-module/index.ts"
    }
  }
}